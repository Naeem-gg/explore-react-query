generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Time {
  id        String   @id @default(cuid())
  name      String   @unique
  fajr      String
  zohr      String
  asr       String
  maghrib   String
  isha      String
  juma      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
